{
  "name": "docker-template/app",
  "type": "project",
  "require": {
    "nikic/fast-route": "^1.3",
    "guzzlehttp/guzzle": "^7.10",
    "phpmailer/phpmailer": "^7.0",
    "stripe/stripe-php": "^19.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\config\\": "config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
